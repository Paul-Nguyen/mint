{
    "harvester": {
        "type": "csv",
        "csv": {
            "fileLocation": "${fascinator.home}/data/Parties_Groups_Sample.csv",
            "idColumn": "ID",
            "recordIDPrefix": "http://fascinator.usq.edu.au/parties/groups/",
            "headerRow": true,
            "delimiter": ",",
            "ignoredFields": [],
            "includedFields" : ["ID", "Title"]
        }
    },
    "indexer": {
        "script": {
            "type": "python",
            "rules": "Parties_Groups.py"
        },
        "params": {
            "repository.name": "Groups",
            "repository.type": "Parties"
        }
    }
}
