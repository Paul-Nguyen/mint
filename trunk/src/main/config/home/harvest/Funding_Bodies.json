{
    "harvester": {
        "type": "csv",
        "csv": {
            "fileLocation": "${fascinator.home}/data/Funding_Bodies_Sample.csv",
            "idColumn": "ID",
            "recordIDPrefix": "http://fascinator.usq.edu.au/funding_bodies/",
            "headerRow": true,
            "headerList": ["ID"],
            "delimiter": ",",
            "ignoredFields": [],
            "includedFields" : ["ID"]
        }
    },
    "indexer": {
        "script": {
            "type": "python",
            "rules": "Funding_Bodies.py"
        },
        "params": {
            "repository.name": "Funding Bodies",
            "repository.type": "Funding Bodies"
        }
    }
}
